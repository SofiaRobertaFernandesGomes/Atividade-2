programa {
  funcao inicio() {
    real a, b, w, x, y, z
    escreva("Digite dois números: ")
    leia(a, b)

    w = a + b
    x = a - b
    y = a * b
    z = a / b

    escreva("w(A soma de a e b) é: ")
    escreva(w)
    escreva("\n")

    escreva("x(A subtração de a e b) é: ")
    escreva(x)
    escreva("\n")

    escreva("y(A mltiplicação de a e b) é: ")
    escreva(y)
    escreva("\n")

    escreva("z(A divisão de a e b) é: ")
    escreva(z)
    escreva("\n")
  }
}
